{
  "name": "godoc",
  "main": "./lib/main",
  "version": "1.1.0",
  "description": "A package for the Go language that provides documentation for identifiers in source code.",
  "keywords": [
    "go",
    "gogetdoc",
    "godoc",
    "go doc",
    "documentation"
  ],
  "author": "Zac Bergquist <zbergquist99@gmail.com> (https://github.com/zmb3)",
  "repository": "https://github.com/zmb3/godoc",
  "bugs": {
    "url": "https://github.com/zmb3/godoc/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.5.3 <2.0.0"
  },
  "dependencies": {
    "atom-package-deps": "4.3.1",
    "etch": "0.7.2"
  },
  "devDependencies": {
    "standard": "^8.3.0",
    "fs-plus": "^2.9.2",
    "temp": "^0.8.3"
  },
  "package-deps": [
    "go-config",
    "go-get"
  ],
  "consumedServices": {
    "go-config": {
      "versions": {
        "1.0.0": "consumeGoconfig"
      }
    },
    "go-get": {
      "versions": {
        "2.0.0": "consumeGoget"
      }
    }
  },
  "providedServices": {
    "go-plus.view": {
      "versions": {
        "0.1.0": "provideGoPlusView"
      }
    }
  },
  "standard": {
    "globals": [
      "atom",
      "waitsForPromise",
      "HTMLElement"
    ]
  }
}

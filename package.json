{
  "name": "godoc",
  "main": "./lib/main",
  "version": "0.3.0",
  "description": "A package for the Go language that provides documentation for identifiers in source code.",
  "keywords": [
    "go",
    "gogetdoc",
    "godoc",
    "go doc",
    "documentation"
  ],
  "author": "Zac Bergquist <zbergquist99@gmail.com> (https://github.com/zmb3)",
  "repository": "https://github.com/zmb3/godoc",
  "bugs": {
    "url": "https://github.com/zmb3/godoc/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.5.3 <2.0.0"
  },
  "dependencies": {
    "atom-package-deps": "^4.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.1",
    "eslint": "^2.11.0",
    "eslint-config-standard": "^5.3.0",
    "eslint-plugin-standard": "^2.0.0",
    "eslint-plugin-promise": "^2.0.0",
    "fs-plus": ">=2.4.0",
    "temp": "^0.8.3"
  },
  "packageDeps": [
    "go-config",
    "go-get"
  ],
  "consumedServices": {
    "go-config": {
      "versions": {
        "1.0.0": "consumeGoconfig"
      }
    },
    "go-get": {
      "versions": {
        "1.0.0": "consumeGoget"
      }
    }
  },
  "standard": {
    "globals": [
      "atom",
      "waitsForPromise"
    ]
  }
}
